    <record id="group_cajeros" model="res.groups">
        <field name="name">Cajeros</field>
        <field name="category_id" ref="base.module_category_accounting_and_finance"/>
        <field name="implied_ids" eval="[(4, ref('account.group_account_invoice')), (4, ref('sales_team.group_sale_salesman_all_leads'))]"/>
    </record>

    <record id="group_cajeros_diario_rule" model="ir.rule">
        <field name="name">Filtro diarios cajero - m√≥dulo pronto</field>
        <field name="model_id" ref="account.model_account_journal"/>
        <field name="domain_force">[('id','in',[14,1])]</field>
        <field name="groups" eval="[(4, ref('group_cajeros'))]"/>
    </record>
   
</odoo>