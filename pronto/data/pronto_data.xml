<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record forcecreate="True" id="decimal_pricelist_fixed_price" model="decimal.precision">
            <field name="name">Pricelist Fixed Price</field>
            <field name="digits">4</field>
        </record>

        <record forcecreate="True" id="actualizar_costo" model="mail.activity.type">
            <field name="name">Actualizar tarifa costo</field>
            <field name="delay_unit">days</field>
            <field name="delay_from">previous_activity</field>
            <field name="icon">fa-phone</field>
            <field name="decoration_type">warning</field>
            <field name="summary">Actualizar tarifa costo</field>
        </record>

        <record forcecreate="True" id="confirmar_fecha_recepcion" model="mail.activity.type">
            <field name="name">Confirmar fecha recepción</field>
            <field name="delay_unit">days</field>
            <field name="delay_from">previous_activity</field>
            <field name="icon">fa-phone</field>
            <field name="decoration_type">warning</field>
            <field name="summary">Confirmar fecha recepción</field>
        </record>

    </data>
</odoo>